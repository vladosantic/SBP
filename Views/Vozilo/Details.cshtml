@model VP.Models.Vozilo

@{
    ViewData["Title"] = "Details of Vozilo";
}

<h1 class="text-warning">Details</h1>
<br/>
<div>
    <h4>Vozilo</h4>
    <hr />
    <dl class="row table-secondary">
        <dt class = "col-sm-2">
            Broj šasije
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.BrojSasije)
        </dd>
        <dt class = "col-sm-2">
            Registracijska oznaka
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.RegistracijskaOznaka)
        </dd>
        <dt class = "col-sm-2">
           Godina proizvodnje
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.GodinaProizvodnje)
        </dd>
        <dt class = "col-sm-2">
            Gorivo
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Gorivo)
        </dd>
        <dt class = "col-sm-2">
            Kategorija
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Kategorija)
        </dd>
        <dt class = "col-sm-2">
            ID lokacije
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.IdLokacijeNavigation.Id)
        </dd>
        <dt class = "col-sm-2">
           Model vozila
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.ModelVozilaNavigation.Id)
        </dd>
        <dt class = "col-sm-2">
           Vrsta vozila
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.VrstaVozilaNavigation.VrstaId)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model?.IdVozila" class="btn btn-outline-success" fdprocessedid="oaubug" >Edit</a> 
    <a asp-action="Index" class="btn btn-outline-light" fdprocessedid="tsh8mj">Back to List</a>
</div>
